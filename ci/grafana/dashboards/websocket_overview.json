{
  "title": "Websocket Overview",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Active Connections",
      "targets": [{"expr": "app_active_connections"}]
    },
    {
      "type": "graph",
      "title": "Messages Total",
      "targets": [{"expr": "rate(app_messages_total[1m])"}]
    },
    {
      "type": "graph",
      "title": "Shutdown Duration",
      "targets": [{"expr": "histogram_quantile(0.95, sum(rate(app_shutdown_duration_seconds_bucket[5m])) by (le))"}]
    }
  ]
}
